{
  "name": "react-hooks-for-backbone",
  "version": "0.6.1",
  "sideEffects": false,
  "description": "React.js Hooks to connect your components with Backbone.js models, events and collections.",
  "cdn": "dist/index.umd.js",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "jsdelivr": "dist/index.umd.js",
  "umd:main": "dist/index.umd.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/",
    "docs/"
  ],
  "devDependencies": {
    "@bitty/deferred": "^0.2.0",
    "@eslint/js": "^9.30.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@testing-library/react": "^16.3.0",
    "@types/backbone": "^1.4.23",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^30.0.0",
    "@types/prettier": "^2.7.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "backbone": "^1.6.1",
    "eslint": "^9.30.1",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup": "^4.44.2",
    "ts-jest": "^29.4.0",
    "ts-jest-resolver": "^2.0.1",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "peerDependencies": {
    "backbone": ">= 1.4",
    "react": ">= 16.8"
  },
  "scripts": {
    "doc": "typedoc src/index.ts",
    "test": "jest",
    "format": "prettier \"**/*.{json,mjs,ts}\" --write",
    "format:check": "prettier \"**/*.{json,mjs,ts}\" --check",
    "lint": "eslint \"**/*.{mjs,ts}\"",
    "lint:fix": "eslint \"**/*.{mjs,ts}\" --fix",
    "build": "rollup --config ./rollup.config.mjs",
    "prepublishOnly": "npm run doc && npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitorLuizC/react-hooks-for-backbone.git"
  },
  "keywords": [],
  "author": {
    "name": "Vitor Luiz Cavalcanti"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VitorLuizC/react-hooks-for-backbone/issues"
  },
  "homepage": "https://github.com/VitorLuizC/react-hooks-for-backbone#readme",
  "dependencies": {
    "@types/jquery": "^3.5.32",
    "@types/lodash.isequal": "^4.5.8",
    "lodash.isequal": "^4.5.0"
  }
}
